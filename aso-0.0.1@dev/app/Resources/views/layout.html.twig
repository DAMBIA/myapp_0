<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8" />
<title>{% block title %}{% endblock %}</title>
<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
<meta name="viewport" content="width=device-width, initialscale=1.0">
{% stylesheets filter='cssrewrite' 
'css/bootstrap.min.css'
'css/aso.css' %}
<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
{% endstylesheets %}
{% block stylesheets %}
{% endblock %}
</head>
<body>
	<div class="container">
		<div class="row"></div>

		{{ render(controller("AsoUtilisateurBundle:Utilisateur:menuGeneral")) }}
	</div>

	{% block body %}
	{% endblock %}

	<div class="row">
		<hr
			class="col-xs-offset-1 col-sm-offset-1 col-md-offset-1 col-lg-offset-1 col-xs-10 col-sm-10 col-md-10 col-lg-10"
			id="hr-footer" />
	</div>
	<div class="row">
		<input type="hidden" name="isSession" id="isSession" value=''
			placeholder="false"
			class="col-xs-12 col-sm-12 col-md-12 col-lg-12 form-control" />
		<footer>aaaa</footer>
	</div>
	</div>
	{% javascripts 'js/jquery-3.1.1.min.js'
	'bundles/fosjsrouting/js/router.js'
	'js/bootstrap.min.js'
	'js/aso.js' 
	'js/layout.js' %}
	<script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}
	<script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
	{% block javascripts %}
	{% endblock %}
</body>
</html>
